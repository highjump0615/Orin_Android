<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@android:color/white" />
            <stroke
                android:color="@android:color/black"
                android:width="1dp" />
            <padding
                android:left="4dp"
                android:top="0dp"
                android:right="4dp"
                android:bottom="0dp" />
        </shape>
    </item>
</layer-list>
